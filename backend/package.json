{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/server.js",
    "docker:start": "sh scripts/docker.sh",
    "test": "jest",
    "dev": "nodemon ./src/server.js",
    "docker:dev": "docker-compose up --b",
    "prettier": "prettier --write 'src/**/*.js'"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "mongoose": "^5.0.11",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "jest": "^22.4.3",
    "nodemon": "^1.12.1",
    "prettier": "^1.11.1"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.js?"
    ],
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "ignore": [
      "__test__/*"
    ],
    "delay": "2500"
  }
}
